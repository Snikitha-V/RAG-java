# Environment variables for RAG Learning Project
# Copy this file to .env and fill in your values
# NEVER commit .env to version control!

# Database password (required)
DB_PASS=your_postgres_password_here

# API key for authentication (optional - if not set, auth is disabled)
API_KEY=your_secret_api_key_here

# LLM server URL (optional - defaults to localhost:8081)
LLM_URL=http://localhost:8081
